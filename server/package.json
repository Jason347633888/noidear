{
  "name": "noidear-server",
  "version": "1.0.0",
  "description": "Document Management System Backend",
  "scripts": {
    "build": "nest build",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "prisma:generate": "prisma generate --schema=src/prisma/schema.prisma",
    "prisma:migrate": "prisma migrate dev --schema=src/prisma/schema.prisma",
    "prisma:seed": "ts-node src/prisma/seed.ts",
    "prisma:reset": "prisma db push --force-reset --schema=src/prisma/schema.prisma && npm run prisma:seed"
  },
  "dependencies": {
    "@elastic/elasticsearch": "^9.3.2",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^4.0.2",
    "@nestjs/core": "^10.0.0",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/schedule": "^6.1.1",
    "@nestjs/swagger": "^7.1.0",
    "@nestjs/throttler": "^6.5.0",
    "@prisma/client": "^5.7.0",
    "@types/ioredis": "^4.28.10",
    "@types/pdfkit": "^0.17.5",
    "@types/pdfmake": "^0.3.1",
    "@willsoto/nestjs-prometheus": "^6.0.2",
    "bcrypt": "^5.1.0",
    "class-transformer": "^0.5.0",
    "class-validator": "^0.14.0",
    "dotenv": "^17.2.3",
    "exceljs": "^4.4.0",
    "helmet": "^8.1.0",
    "ioredis": "^5.9.3",
    "jsonwebtoken": "^9.0.0",
    "ldapjs": "^3.0.7",
    "minio": "^8.0.6",
    "nanoid": "^3.3.11",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "pdfmake": "^0.3.4",
    "prom-client": "^15.1.3",
    "reflect-metadata": "^0.1.13",
    "rxjs": "^7.8.1",
    "safe-regex": "^2.1.1",
    "sharp": "^0.34.5"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.22",
    "@types/bcrypt": "^5.0.0",
    "@types/express": "^4.17.17",
    "@types/helmet": "^0.0.48",
    "@types/jest": "^30.0.0",
    "@types/jsonwebtoken": "^9.0.0",
    "@types/ldapjs": "^3.0.6",
    "@types/multer": "^2.0.0",
    "@types/node": "^20.19.30",
    "@types/passport-jwt": "^4.0.1",
    "@types/safe-regex": "^1.1.6",
    "@types/sharp": "^0.31.1",
    "@types/supertest": "^6.0.3",
    "jest": "^30.2.0",
    "prisma": "^5.7.0",
    "supertest": "^7.2.2",
    "ts-jest": "^29.4.6",
    "typescript": "^5.3.0"
  }
}
